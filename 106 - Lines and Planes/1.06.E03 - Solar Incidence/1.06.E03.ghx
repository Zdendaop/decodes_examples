                    <item name="CodeInput" type_name="gh_string" type_code="10">from decodes.extensions.solar_geometry import *

# find the normals for each mesh face
normals = [mesh.face_normal(i) for i in range(len(mesh.faces))]

# generate sun vector for a given location and time 
latitude, longitude, timezone = 41.73, -71.43, -5.0 #Providence, RI
#latitude, longitude, timezone = -0.23, -78.52, -5.0 #Quito, Ecuador

sg = SolarGeom(latitude, longitude, timezone)
day = sg.str_to_day_of_year("8/23")
hour = sg.str_to_decimal_hour("09:00")
sun_vec = sg.vec_at(day, hour)


vals = []  #houses the cosine of the angle of incidence for each mesh face


for normal in normals:
    cos_angle_incidence = sun_vec.dot(normal)
    if cos_angle_incidence &lt; 0: cos_angle_incidence = 0
    vals.append(cos_angle_incidence)

#graphics, generating individually colored mesh faces

faces = []

eps = 3.0/360 #avoid black which will show up as white in Rhino
remap_vals = [Interval.remap(vals[i],Interval(0,1-eps)) for i in range(len(vals))]

for i, msh in enumerate(Mesh.explode(mesh)):
    msh.set_color(Color.HSB(0.5, vals[i], vals[i]))
    #msh.set_color(Color.HSB(0.5, remap_vals[i], remap_vals[i])) 
    faces.append(msh)
    
out.put(faces)



