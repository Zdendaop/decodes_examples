                    <item name="CodeInput" type_name="gh_string" type_code="10">from decodes.extensions.solar_geometry import *

#fixed plane for which we will be calculating angle of incidence with sun vectors
from math import cos, sin, radians

phi = radians(angle_1)
theta = radians(angle_2)
normal_vec = Vec(sin(phi)*cos(theta), sin(phi)*sin(theta), cos(phi))

pln = Plane(Point(), normal_vec)
a.put(pln)

#generates the sun position for a given location
sun_pts = [] 
latitude, longitude, timezone = 41.73, -71.43, -5.0 #Providence, RI
sg = SolarGeom(latitude, longitude, timezone)

#stores cosine of the angle of incidence for every hour of year
vals = [] 
days = range(0, 365, 5)
hours = range(24)
amp = 20

for hour in hours:
    for day in days:
        sun_vec = sg.vec_at(day, hour)
        sun_pts.append(Point(sun_vec)*amp)
        cos_angle_incidence = sun_vec.dot(pln.normal)
        if cos_angle_incidence &lt; 0: cos_angle_incidence = 0
        vals.append(cos_angle_incidence)


#graphics, generating colored analema lines
num_days = len(days)
num_hours = len(hours)

analema_lines = []
for i in range(num_hours):
    start = i*num_days
    for j in range(num_days-1):
        index = start + j
        seg = Segment(sun_pts[index], sun_pts[index+1])
        seg.set_color(Color.HSB(0.84, 0.75, vals[index]))
        analema_lines.append(seg)
        
        
out.put(analema_lines)

