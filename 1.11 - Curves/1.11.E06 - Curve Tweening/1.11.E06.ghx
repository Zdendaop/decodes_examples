                    <item name="CodeInput" type_name="gh_string" type_code="10">from math import *
half_pi = pi/2
two_pi = pi*2

pa = crv_a.eval(t)
pb = crv_b.eval(t)

a.put(Segment(pa,pb))

"""
demonstrates the projection onto the bounding lines of a rectangle
"""
rec_w = 8
rec_h = 4

t = t*pi*2
if t != 0 : 
    x_coord = 0.5 * rec_h * cos(t)/sin(t)
    print x_coord
    if abs(x_coord) &lt; 10: b.put(Point(x_coord,rec_h/2))
if t != half_pi : 
    y_coord = 0.5 * rec_w * sin(t)/cos(t)
    print y_coord
    if abs(y_coord) &lt; 10: b.put(Point(rec_w/2,y_coord))
</item>
