                    <item name="CodeInput" type_name="gh_string" type_code="10">from math import *
two_pi = 2*pi
half_pi = 0.5*pi
#########################################

def func(t):
    sub_ivals = Interval()//4
    
    # point on right edge
    if t in sub_ivals[0]:
        x = rec_w/2
        y = Interval.remap(t, sub_ivals[0], Interval(-rec_h/2,rec_h/2) )
    # point on top edge
    if t in sub_ivals[1]:
        x = Interval.remap(t, sub_ivals[1], Interval(rec_w/2,-rec_w/2))
        y = rec_h/2
    # point on left edge
    if t in sub_ivals[2]:
        x = -rec_w/2
        y = Interval.remap(t, sub_ivals[2], Interval(rec_h/2,-rec_h/2) )
    # point on bottom edge
    if t in sub_ivals[3]:
        x = Interval.remap(t, sub_ivals[3], Interval(-rec_w/2,rec_w/2))
        y = -rec_h/2
        
    return Point(x,y)


#########################################
crv = Curve(func,Interval())
a.put(crv)
crv.tol = 0.01
b.put(crv.surrogate)
