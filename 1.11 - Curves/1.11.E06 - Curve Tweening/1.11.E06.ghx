                    <item name="CodeInput" type_name="gh_string" type_code="10">from math import *
two_pi = 2*pi
half_pi = 0.5*pi
#########################################

"""
Tweened Curve
Defines a curve that interpolates between crv_a and crv_b at parameter m
"""
def func(t):
    return crv_a.eval(t)*(1-m) + crv_b.eval(t)*m 

crv = Curve(func,Interval())


#########################################
crv.tol = 0.005
a.put(crv.surrogate)

res_u, res_v = 3, 32

pt_grid = [[] for r in range(res_v)]

for mm in Interval().divide(res_u,True):
    def func(t):
        return crv_a.eval(t)*(1-mm) + crv_b.eval(t)*mm 
    crv = Curve(func,Interval())
    
    for v,t in enumerate(Interval()/res_v):
        pt_grid[v].append(crv.eval(t))
        

for pts in pt_grid:
    b.put(PLine(pts)) </item>
