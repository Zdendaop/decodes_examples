                    <item name="CodeInput" type_name="gh_string" type_code="10">from math import *
two_pi = 2*pi
half_pi = 0.5*pi
#########################################

def func(t):
    # projections onto the bounding lines of the rectangle
    if t != 0 : proj_x = 0.5 * rec_h * cos(t)/sin(t)
    if t != half_pi : proj_y = 0.5 * rec_w * sin(t)/cos(t)
    
    # an angle related to the aspect ratio of this rectangle
    atn = atan(rec_h/rec_w)
    
    # point on top half of right edge
    if t in Interval(0,atn): 
        x = rec_w/2
        y = proj_y
    # point on top edge
    elif t in Interval(atn,pi-atn):
        x = proj_x
        y = rec_h/2
    # point on left edge
    elif t in Interval(pi-atn,pi+atn):  
        x = -rec_w/2
        y = -proj_y
    # point on bottom edge
    elif t in Interval(pi+atn,two_pi-atn):
        x = -proj_x
        y = -rec_h/2
    # point on bottom half of right edge
    elif t in Interval(two_pi-atn,two_pi): 
        x = rec_w/2
        y = proj_y
    
    return Point(x,y)

#########################################
crv = Curve(func,Interval.twopi())
a.put(crv)
crv.tol = 0.01
b.put(crv.surrogate)
