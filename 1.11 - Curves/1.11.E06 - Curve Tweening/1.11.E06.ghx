                    <item name="CodeInput" type_name="gh_string" type_code="10">from math import *
two_pi = 2*pi
half_pi = 0.5*pi
#########################################

def func(t):
    t_value = two_pi*t
    c = cos(t_value)
    s = sin(t_value)
    c_n = cos((num_cusps-1)*t_value)
    s_n = sin((num_cusps-1)*t_value)
    factor = rad/num_cusps
    x = factor*((num_cusps-1)*c - c_n)
    y = factor*((num_cusps-1)*s + s_n)
    return Point(x,y)
    

ival = Interval()

#########################################
crv = Curve(func,ival,0.01)
a.put(crv)
b.put(crv.surrogate)</item>
