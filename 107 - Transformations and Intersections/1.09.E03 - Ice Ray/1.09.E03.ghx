def limit(ray, segs=[]):
    vert_pln = Plane(ray.spt,ray.vec.cross(UZ))
    xsec = Intersector()
    if not xsec.of(bound,vert_pln) or len(xsec) != 2: 
        raise Exception("intersection failed")
    
    results = []
    for pt in xsec:
        near = ray.near(pt)
        if near[1]&gt;0: results.append( (near[1],pt) )
    if len(results) == 2: return Segment(results[0][1],results[1][1])
    
    for seg in segs:
        if xsec.of(ray,seg): results.append((xsec.ta, xsec[0]))
    
    results.sort()
    #print results
    if len(results) == 1: return Segment(ray.spt,results[0][1])
    if len(results) == 2: return Segment(ray.spt,results[1][1])
    return Segment(results[0][1],results[2][1])


