                    <item name="CodeInput" type_name="gh_string" type_code="10">from decodes.extensions.graph import SpatialGraph
pgon_limit = 100

gph = SpatialGraph()
for seg in segs: gph.add_edge(seg.spt,seg.ept)

    
def rightiness(cvec,nvec):
    ang = cvec.angle(nvec)
    if round(ang,6) == round(math.pi,6) :
        return 999
    if cvec.cross(nvec).z &gt;=0 : return ang
    else: return -ang

nds = gph.nodes.copy()
walked_edges = set()
while len(nds) &gt; 0:
    st_spt = nds.pop()
    for st_ept in gph.edges[st_spt]:
        wpts = [st_spt,st_ept]
        b.put([st_spt,st_ept,Circle(Plane(st_spt),0.1)])
        if not (st_spt,st_ept) in walked_edges:
            walking = True
            found_dup_edge = False
            n = 0
            while walking and n &lt; pgon_limit:
                print n
                for cpt in gph.edges[wpts[-1]]: cpt.r = rightiness(Vec(wpts[-2],wpts[-1]),Vec(wpts[-1],cpt))
                choice = sorted(gph.edges[wpts[-1]], key=lambda cpt: cpt.r)[0]
                # print choice, choice.r
                #if choice.r == 999: print gph.edges[wpts[-1]]
                if choice is wpts[0] : walking = False # got back home
                else: wpts.append(choice)
                n += 1
                if (wpts[-2],wpts[-1]) in walked_edges:
                    found_dup_edge = True
                    break
                
            if len(wpts) &gt; 2 and not found_dup_edge:
                try:
                    pg = PGon(wpts)
                    for n in range(len(wpts)-1): walked_edges.add((wpts[n],wpts[n+1]))
                    #b.put(wpts)
                    a.put(pg)
                except:
                    print "COULD NOT CREATE PGON - are all of these shapes closed?"

