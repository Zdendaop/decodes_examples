                    <item name="CodeInput" type_name="gh_string" type_code="10">
#########################################

def shatter_segments_at_endpoints(segs):
    pts = Point.cull_duplicates([s.spt for s in segs]+[s.ept for s in segs])
    ret = []
    for seg in segs:
        rslts = []
        for pt in pts:
            near = seg.near(pt)
            if near[2] &lt; EPSILON: rslts.append( (near[1],pt) )
        
        rslts.sort()
        for a,b in zip(rslts[:-1],rslts[1:]): ret.append( Segment(a[1],b[1]) )
    return ret
    
a.put(shatter_segments_at_endpoints(segs))

