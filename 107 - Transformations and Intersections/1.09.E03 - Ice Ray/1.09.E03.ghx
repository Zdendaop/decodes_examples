rad = 10.0
bound = Circle(Plane(Point(),UZ),rad)
b.put(bound)

def limit(ray, segs):
    vert_pln = Plane(ray.spt,ray.vec.cross(UZ))
    xsec = Intersector()
    if xsec.of(bound,vert_pln) and len(xsec) == 2:
        pa, ta, da = ray.near(xsec[0])
        pb, tb, db = ray.near(xsec[1])
        if ta &gt; 0 and tb &gt; 0 : return Segment(pa,pb)
        if ta &gt; 0 : return Segment(ray.spt,pa)
        if tb &gt; 0 : return Segment(ray.spt,pb)
    raise Exception("intersection failed")
    

seg = limit( seed_line , [] )
a.put(seg)

lib = [
    {"t":0.25, "xf":Xform.rotation(angle=0.9)},
    {"t":0.5, "xf":Xform.rotation(angle=-3.9)}
]

nseg = limit(Ray(seg.eval(lib[0]["t"]),seg.vec.cross(UZ)*lib[0]["xf"]))
a.put(nseg)
nseg = limit(Ray(seg.eval(lib[1]["t"]),seg.vec.cross(UZ)*lib[1]["xf"]))
