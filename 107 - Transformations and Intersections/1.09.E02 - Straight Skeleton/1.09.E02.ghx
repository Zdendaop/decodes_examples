              <items count="3">
                <item name="GUID" type_name="gh_guid" type_code="9">fccad19e-dcb6-44ab-8edb-54dd6ab7e966</item>
                <item name="Lib" type_name="gh_guid" type_code="9">30f802e3-1f17-2416-d927-d82433eca0a2</item>
                <item name="Name" type_name="gh_string" type_code="10">Decodes Python Script</item>
              </items>
              <chunks count="1">
                <chunk name="Container">
                  <items count="8">
                    <item name="CodeInput" type_name="gh_string" type_code="10">
BIG = 1/EPSILON

edges = pg.edges
rays = [Ray(pg.pts[n],pg.angle_bisector(n)[0].normalized()) for n in range(len(pg))]
for ry in rays:
    if not ry.spt+ry.vec in pg: ry.vec = ry.vec.inverted()

def appx_less_than(a,b):
    return a &lt; b or a-b &lt; EPSILON


xsec = Intersector()
cnt = 0
while True:
    if cnt &gt;= lmt: break    
    if len(rays) &lt;= 2:
        print "only ", len(rays), "rays left, all done"
        sc = Segment(rays[0].spt,rays[1].spt)
        c.put(sc)
        break
    
    xsec_prms = []
    # for each ray, a tuple of the t-val of intersection with neighbors (prev,next)
    for ry_this, ry_prev, ry_next in match(rays,[0,-1,1]):
        t_prev, t_next = False, False
        if xsec.of(ry_this, ry_prev): t_prev = xsec.ta
        if xsec.of(ry_this, ry_next): t_next = xsec.ta
        xsec_prms.append({'prev':t_prev,'next':t_next})
        #c.put(ry_this.eval(min(tp,tn)))
        
    reducables = []
    for n, pair in enumerate(match(xsec_prms,[0,-1])):
        this_prms, prev_prms = pair
        # if this ray does not intersect the previous, continue
        if not this_prms['prev']: continue
        # if this ray intersects the previous before the next
        if not this_prms['next'] or appx_less_than(this_prms['prev'],this_prms['next']):
            # if the previous intersects this one before its other neighbor
            if not prev_prms['prev'] or appx_less_than(prev_prms['next'], prev_prms['prev']):
                # this is an appropriate ray to reduce. log it.
                rank = this_prms['prev'] + prev_prms['next']
                reducables.append( (rank,{'param':this_prms['prev'],'idx':n}) )
    
    for reducable in reducables:
        t,idx = reducable[1]['param'], reducable[1]['idx']
        pt = rays[idx].eval(t)
        sa = Segment(rays[idx].spt,pt)
        sb = Segment(rays[idx-1].spt,pt)
        d.put(sa)
        d.put(sb)        
    
    if len(reducables)&gt;0:
        choice = sorted(reducables)[0][1]
        t,idx = choice['param'], choice['idx']
        pt = rays[idx].eval(t)
        print "reducing at index: ", idx
        
        sa = Segment(rays[idx].spt,pt)
        sb = Segment(rays[idx-1].spt,pt)
        c.put(sa)
        c.put(sb)
        
        print edges[idx].vec, edges[idx-2].vec
        
        # figuring out which of the possible bisectors to use is a real pain.
        if edges[idx].is_parallel(edges[idx-2]): bsecs = [edges[idx].vec.normalized(), edges[idx-2].vec.normalized()]
        else: bsecs = Vec.bisectors(edges[idx].vec, edges[idx-2].vec)
        try:
            guide = Vec.average([sa.vec.normalized(),sb.vec.normalized()])
            #guide = edges[idx].vec
            #guide = Vec(pt,Point.centroid([e.eval(0.5) for e in edges]))
            #guide = edges[idx-1].vec.cross(Vec(0,0,1))
            vec = guide.best_match(bsecs)
        except:
            print "i don't do well with symmetry"
            break
        
        
        edges.pop(idx-1)
        rays[idx] = Ray(pt,vec)
        rays.pop(idx-1)
            
    else:
        print "CANT REDUCE ANY?!"
        break
    
