"""
Offset Routine Handling Edge Event
Resolves edge events which occurs when angle bisectors cross 
"""
def offset(self,offset_dist,flip=False):
    vecs, angles = zip(*[self.angle_bisector(i) for i in range(len(self))])
    off_dists = [offset_dist/math.sin(angle) for angle in angles]
    if flip: off_dists = [-d for d in off_dists]
    bones = [Segment(p, p + v.normalized(d)) for v,d,p in zip(vecs,off_dists,self.pts) ]
    dead_bones = []
    while True:
        still_cleaning = clean_crossed_bones(bones)
        if not still_cleaning: break
        else: 
            bones = still_cleaning[0]
            dead_bones.extend(still_cleaning[1])
        
    b.put(bones)
    c.put(dead_bones)
    return PGon([bone.ept for bone in bones])

def clean_crossed_bones(bones):
    xsec = Intersector()
    for n, pair in enumerate(match(bones,[0,-1])):
        bone, other = pair
        if xsec.of(bone,other):
            va,vb = bone.vec*(1-xsec.ta), other.vec*(1-xsec.tb)
            bones[n] = Segment(xsec[0],Vec.average([va,vb]))
            bones.pop(n-1)
            return bones, [Segment(bone.spt,xsec[0]),Segment(other.spt,xsec[0])]
    return False


off = offset(pg,val)
a.put(off.edges)</item>
