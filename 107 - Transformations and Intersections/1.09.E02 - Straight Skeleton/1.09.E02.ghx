edges = pg.edges
rays = [Ray(pg.pts[n],pg.angle_bisector(n)[0].normalized()) for n in range(len(pg))]
for ry in rays:
    if not ry.spt+ry.vec in pg: ry.vec = ry.vec.inverted()

def appx_less_than(a,b):
    return a &lt; b or a-b &lt; EPSILON

cnt,lmt = 0,100
#########################################

xsec = Intersector()
nice_bones = []
while True:
    # at each step, we reduce.
    # if only two rays remain, we may connect them and be done.
    if len(rays) &lt;= 2:
        nice_bones.append( Segment(rays[0].spt,rays[1].spt) )
        break
    
    xsec_prms = []
    # for each ray, a tuple of the t-val of intersection with neighbors (prev,next)
    for ry_this, ry_prev, ry_next in match(rays,[0,-1,1]):
        t_prev, t_next = False, False
        if xsec.of(ry_this, ry_prev): t_prev = xsec.ta
        if xsec.of(ry_this, ry_next): t_next = xsec.ta
        xsec_prms.append({'prev':t_prev,'next':t_next})
        
        
    # for this step, produce a list of rays that may be reduced. 
    # for each ray, a distance to the relevant intersection and the index of the ray (dist,index)
    reducables = []
    for n, pair in enumerate(match(xsec_prms,[0,-1])):
        this_prms, prev_prms = pair
        # if this ray does not intersect the previous, continue
        if not this_prms['prev']: continue
        # if this ray intersects the previous before the next
        if not this_prms['next'] or appx_less_than(this_prms['prev'],this_prms['next']):
            # if the previous intersects this one before its other neighbor
            if not prev_prms['prev'] or appx_less_than(prev_prms['next'], prev_prms['prev']):
                # this is an appropriate ray to reduce. log it.
                rank = this_prms['prev'] + prev_prms['next']
                reducables.append( (rank,{'param':this_prms['prev'],'idx':n}) )
    
    if len(reducables)&gt;0:
        choice = sorted(reducables)[0][1]
        t,idx = choice['param'], choice['idx']
        pt = rays[idx].eval(t)
        sa,sb = Segment(rays[idx].spt,pt), Segment(rays[idx-1].spt,pt)
        nice_bones.extend([sa,sb])
        
        # figuring out which of the possible bisectors to use is a real pain.
        if edges[idx].is_parallel(edges[idx-2]):
            # in case of parallel edges 
            bsecs = [edges[idx].vec.normalized(), edges[idx-2].vec.normalized()]
        else: 
            bsecs = Vec.bisectors(edges[idx].vec, edges[idx-2].vec)
        
        guide = Vec.bisector(sa.vec,sb.vec)
        vec = guide.best_match(bsecs)
        
        edges.pop(idx-1)
        rays[idx] = Ray(pt,vec)
        rays.pop(idx-1)
            
    else:
        print "CANT REDUCE ANY?!"
        break

#########################################
    
    cnt += 1
    if cnt &gt;= lmt: break

a.put(nice_bones)
