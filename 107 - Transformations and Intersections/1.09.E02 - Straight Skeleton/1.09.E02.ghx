#########################################


"""
Simple Offset
Allows bisectors to cross each other, and allows polygon to become self-intersecting.
"""
def offset(self,dist,flip=False):
    # NOTE THE SPLAT OPERATOR
    vecs, angles = zip(*[self.angle_bisector(i) for i in range(len(self))])
    off_dists = [dist/math.sin(angle) for angle in angles]
    if flip: off_dists = [-d for d in off_dists]
    bones = [Segment(p, p + v.normalized(d)) for v,d,p in zip(vecs,off_dists,self.pts) ]
    
    b.put(bones)
    
    return PGon([bone.ept for bone in bones])


#########################################

off = offset(pg,val)
a.put(off.edges)</item>
